{
  "extends": ["./.wxt/tsconfig.json", "@tsconfig/svelte/tsconfig.json"],
  "compilerOptions": {
    "useDefineForClassFields": true,
    "baseUrl": ".",
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],
      "@": ["../src"],
      "@/*": ["../src/*"],
      "~": ["../src"],
      "~/*": ["../src/*"],
      "@@": [".."],
      "@@/*": ["../*"],
      "~~": [".."],
      "~~/*": ["../*"]
    }
  }
}
